    bottom  : 0;
  }

  .slider-thumbnail {
    position       : relative;
    height         : 100%;
    display        : flex;
    justify-content: flex-start;
    background     : white;
    border-radius  : 4px;
    overflow       : hidden;
    white-space    : normal;

