      font-size      : 20px;
      padding        : 1.5rem;
      /* font-family : 'Roboto Condensed'; */
      font-family    : Roboto;
      flex-grow      : 1;
      line-height    : 1;
      text-align     : left;
    }
  }

  .slider-slide {
    .slide-img {
      height             : 780px;
      background-size    : cover;
      background-position: center;
    }

    .slide-body {
      position      : absolute;
      // bottom     : 40%;
      bottom        : 30%;
      max-width     : 600px;
      margin-left   : 4rem;
      color         : white;

      .slide-title {
        font-size    : 3rem;
        font-weight  : bold;
        margin-bottom: 3rem;
        line-height  : 1.2;
      }

      .slide-description {
        font-size  : 1.2rem;
        line-height: 1.2;
        font-weight: 200;
        opacity    : 0.8;
      }
    }
  }

}


